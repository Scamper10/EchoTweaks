{
	"java.configuration.updateBuildConfiguration": "automatic",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"build/**": true,
		"run/**": true,
		"gradle/**": true,
		"gradlew": true,
		// "gradlew.bat": true, // needed for tasks tab to show
		"*.gradle": true
	},
	"java.debug.settings.onBuildFailureProceed": true
}
